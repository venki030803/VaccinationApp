<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="415dp"
        android:layout_height="188dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bgallbox" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Menu"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.076" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="28dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.861"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.088"
        app:srcCompat="@drawable/notification" />


    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="420dp"
        android:layout_height="475dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/boxbutton1" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.821">

        <LinearLayout
            android:id="@+id/frame1"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="482dp">

            <ImageView
                android:id="@+id/proimg"
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/user"
                app:srcCompat="@drawable/user" />

            <TextView
                android:id="@+id/pro"

                android:layout_width="274dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Profile"
                android:textColor="@color/black"
                android:textSize="24sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/frame2"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="556dp">

            <ImageView
                android:id="@+id/notimg"
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                app:srcCompat="@drawable/bell" />

            <TextView
                android:id="@+id/textView141"
                android:layout_width="275dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Notification"
                android:textColor="@color/black"
                android:textSize="24sp" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/fram3"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="634dp">

            <ImageView
                android:id="@+id/faqimg"
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/faq"
                app:srcCompat="@drawable/faq" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="273dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="Faq"
                android:textColor="@color/black"
                android:textSize="24sp" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/frame4"
            android:layout_width="379dp"
            android:layout_height="44dp"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="714dp">

            <ImageView
                android:id="@+id/logout"
                android:layout_width="68dp"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:src="@drawable/faq"
                app:srcCompat="@drawable/exit" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="273dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/poppins_semibold"
                android:paddingLeft="20dp"
                android:paddingTop="3dp"
                android:text="LogOut"
                android:textColor="@color/black"
                android:textSize="24sp" />


        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="50dp"
        android:layout_height="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.396"
        app:srcCompat="@color/black" />

</androidx.constraintlayout.widget.ConstraintLayout>